#!/usr/bin/env bash
#
# Outputs the binary digit that corresponds to the given ascii character.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore ASCII da convertire in binario.\\e[0m"

case "$1" in
  'NUL')   printf '%s\n' "0"       ;;
  'SOH')   printf '%s\n' "1"       ;;
  'STX')   printf '%s\n' "10"      ;;
  'ETX')   printf '%s\n' "11"      ;;
  'EOT')   printf '%s\n' "100"     ;;
  'ENQ')   printf '%s\n' "101"     ;;
  'ACK')   printf '%s\n' "110"     ;;
  'BEL')   printf '%s\n' "111"     ;;
  'BS')    printf '%s\n' "1000"    ;;
  'TAB')   printf '%s\n' "1001"    ;;
  'LF')    printf '%s\n' "1010"    ;;
  'VT')    printf '%s\n' "1011"    ;;
  'FF')    printf '%s\n' "1100"    ;;
  'CR')    printf '%s\n' "1101"    ;;
  'SO')    printf '%s\n' "1110"    ;;
  'SI')    printf '%s\n' "1111"    ;;
  'DLE')   printf '%s\n' "10000"   ;;
  'DC1')   printf '%s\n' "10001"   ;;
  'DC2')   printf '%s\n' "10010"   ;;
  'DC3')   printf '%s\n' "10011"   ;;
  'DC4')   printf '%s\n' "10100"   ;;
  'NAK')   printf '%s\n' "10101"   ;;
  'SYN')   printf '%s\n' "10110"   ;;
  'ETB')   printf '%s\n' "10111"   ;;
  'CAN')   printf '%s\n' "11000"   ;;
  'EM')    printf '%s\n' "11001"   ;;
  'SUB')   printf '%s\n' "11010"   ;;
  'ESC')   printf '%s\n' "11011"   ;;
  'FS')    printf '%s\n' "11100"   ;;
  'GS')    printf '%s\n' "11101"   ;;
  'RS')    printf '%s\n' "11110"   ;;
  'US')    printf '%s\n' "11111"   ;;
  'Space') printf '%s\n' "100000"  ;;
  '!')     printf '%s\n' "100001"  ;;
  '\"')    printf '%s\n' "100010"  ;;
  '#')     printf '%s\n' "100011"  ;;
  '$')     printf '%s\n' "100100"  ;;
  '%')     printf '%s\n' "100101"  ;;
  '&')     printf '%s\n' "100110"  ;;
  "'")     printf '%s\n' "100111"  ;;
  '(')     printf '%s\n' "101000"  ;;
  ')')     printf '%s\n' "101001"  ;;
  '*')     printf '%s\n' "101010"  ;;
  '+')     printf '%s\n' "101011"  ;;
  ',')     printf '%s\n' "101100"  ;;
  '-')     printf '%s\n' "101101"  ;;
  '.')     printf '%s\n' "101110"  ;;
  '/')     printf '%s\n' "101111"  ;;
  '0')     printf '%s\n' "110000"  ;;
  '1')     printf '%s\n' "110001"  ;;
  '2')     printf '%s\n' "110010"  ;;
  '3')     printf '%s\n' "110011"  ;;
  '4')     printf '%s\n' "110100"  ;;
  '5')     printf '%s\n' "110101"  ;;
  '6')     printf '%s\n' "110110"  ;;
  '7')     printf '%s\n' "110111"  ;;
  '8')     printf '%s\n' "111000"  ;;
  '9')     printf '%s\n' "111001"  ;;
  ':')     printf '%s\n' "111010"  ;;
  ';')     printf '%s\n' "111011"  ;;
  '<')     printf '%s\n' "111100"  ;;
  '=')     printf '%s\n' "111101"  ;;
  '>')     printf '%s\n' "111110"  ;;
  '?')     printf '%s\n' "111111"  ;;
  '@')     printf '%s\n' "1000000" ;;
  'A')     printf '%s\n' "1000001" ;;
  'B')     printf '%s\n' "1000010" ;;
  'C')     printf '%s\n' "1000011" ;;
  'D')     printf '%s\n' "1000100" ;;
  'E')     printf '%s\n' "1000101" ;;
  'F')     printf '%s\n' "1000110" ;;
  'G')     printf '%s\n' "1000111" ;;
  'H')     printf '%s\n' "1001000" ;;
  'I')     printf '%s\n' "1001001" ;;
  'J')     printf '%s\n' "1001010" ;;
  'K')     printf '%s\n' "1001011" ;;
  'L')     printf '%s\n' "1001100" ;;
  'M')     printf '%s\n' "1001101" ;;
  'N')     printf '%s\n' "1001110" ;;
  'O')     printf '%s\n' "1001111" ;;
  'P')     printf '%s\n' "1010000" ;;
  'Q')     printf '%s\n' "1010001" ;;
  'R')     printf '%s\n' "1010010" ;;
  'S')     printf '%s\n' "1010011" ;;
  'T')     printf '%s\n' "1010100" ;;
  'U')     printf '%s\n' "1010101" ;;
  'V')     printf '%s\n' "1010110" ;;
  'W')     printf '%s\n' "1010111" ;;
  'X')     printf '%s\n' "1011000" ;;
  'Y')     printf '%s\n' "1011001" ;;
  'Z')     printf '%s\n' "1011010" ;;
  '[')     printf '%s\n' "1011011" ;;
  \\)      printf '%s\n' "1011100" ;;
  ']')     printf '%s\n' "1011101" ;;
  '^')     printf '%s\n' "1011110" ;;
  '_')     printf '%s\n' "1011111" ;;
  '\`')    printf '%s\n' "1100000" ;;
  'a')     printf '%s\n' "1100001" ;;
  'b')     printf '%s\n' "1100010" ;;
  'c')     printf '%s\n' "1100011" ;;
  'd')     printf '%s\n' "1100100" ;;
  'e')     printf '%s\n' "1100101" ;;
  'f')     printf '%s\n' "1100110" ;;
  'g')     printf '%s\n' "1100111" ;;
  'h')     printf '%s\n' "1101000" ;;
  'i')     printf '%s\n' "1101001" ;;
  'j')     printf '%s\n' "1101010" ;;
  'k')     printf '%s\n' "1101011" ;;
  'l')     printf '%s\n' "1101100" ;;
  'm')     printf '%s\n' "1101101" ;;
  'n')     printf '%s\n' "1101110" ;;
  'o')     printf '%s\n' "1101111" ;;
  'p')     printf '%s\n' "1110000" ;;
  'q')     printf '%s\n' "1110001" ;;
  'r')     printf '%s\n' "1110010" ;;
  's')     printf '%s\n' "1110011" ;;
  't')     printf '%s\n' "1110100" ;;
  'u')     printf '%s\n' "1110101" ;;
  'v')     printf '%s\n' "1110110" ;;
  'w')     printf '%s\n' "1110111" ;;
  'x')     printf '%s\n' "1111000" ;;
  'y')     printf '%s\n' "1111001" ;;
  'z')     printf '%s\n' "1111010" ;;
  '{')     printf '%s\n' "1111011" ;;
  '|')     printf '%s\n' "1111100" ;;
  '}')     printf '%s\n' "1111101" ;;
  '~')     printf '%s\n' "1111110" ;;
  'DEL')   printf '%s\n' "1111111" ;;
esac
