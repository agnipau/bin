#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given binary value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore binario da convertire in ASCII.\\e[0m"

case "$1" in
  '0')       printf '%s\n' "NUL"   ;;
  '1')       printf '%s\n' "SOH"   ;;
  '10')      printf '%s\n' "STX"   ;;
  '11')      printf '%s\n' "ETX"   ;;
  '100')     printf '%s\n' "EOT"   ;;
  '101')     printf '%s\n' "ENQ"   ;;
  '110')     printf '%s\n' "ACK"   ;;
  '111')     printf '%s\n' "BEL"   ;;
  '1000')    printf '%s\n' "BS"    ;;
  '1001')    printf '%s\n' "TAB"   ;;
  '1010')    printf '%s\n' "LF"    ;;
  '1011')    printf '%s\n' "VT"    ;;
  '1100')    printf '%s\n' "FF"    ;;
  '1101')    printf '%s\n' "CR"    ;;
  '1110')    printf '%s\n' "SO"    ;;
  '1111')    printf '%s\n' "SI"    ;;
  '10000')   printf '%s\n' "DLE"   ;;
  '10001')   printf '%s\n' "DC1"   ;;
  '10010')   printf '%s\n' "DC2"   ;;
  '10011')   printf '%s\n' "DC3"   ;;
  '10100')   printf '%s\n' "DC4"   ;;
  '10101')   printf '%s\n' "NAK"   ;;
  '10110')   printf '%s\n' "SYN"   ;;
  '10111')   printf '%s\n' "ETB"   ;;
  '11000')   printf '%s\n' "CAN"   ;;
  '11001')   printf '%s\n' "EM"    ;;
  '11010')   printf '%s\n' "SUB"   ;;
  '11011')   printf '%s\n' "ESC"   ;;
  '11100')   printf '%s\n' "FS"    ;;
  '11101')   printf '%s\n' "GS"    ;;
  '11110')   printf '%s\n' "RS"    ;;
  '11111')   printf '%s\n' "US"    ;;
  '100000')  printf '%s\n' "Space" ;;
  '100001')  printf '%s\n' "!"     ;;
  '100010')  printf '%s\n' "\""    ;;
  '100011')  printf '%s\n' "#"     ;;
  '100100')  printf '%s\n' "$"     ;;
  '100101')  printf '%s\n' "%"     ;;
  '100110')  printf '%s\n' "&"     ;;
  '100111')  printf '%s\n' "'"     ;;
  '101000')  printf '%s\n' "("     ;;
  '101001')  printf '%s\n' ")"     ;;
  '101010')  printf '%s\n' "*"     ;;
  '101011')  printf '%s\n' "+"     ;;
  '101100')  printf '%s\n' ","     ;;
  '101101')  printf '%s\n' "-"     ;;
  '101110')  printf '%s\n' "."     ;;
  '101111')  printf '%s\n' "/"     ;;
  '110000')  printf '%s\n' "0"     ;;
  '110001')  printf '%s\n' "1"     ;;
  '110010')  printf '%s\n' "2"     ;;
  '110011')  printf '%s\n' "3"     ;;
  '110100')  printf '%s\n' "4"     ;;
  '110101')  printf '%s\n' "5"     ;;
  '110110')  printf '%s\n' "6"     ;;
  '110111')  printf '%s\n' "7"     ;;
  '111000')  printf '%s\n' "8"     ;;
  '111001')  printf '%s\n' "9"     ;;
  '111010')  printf '%s\n' ":"     ;;
  '111011')  printf '%s\n' ";"     ;;
  '111100')  printf '%s\n' "<"     ;;
  '111101')  printf '%s\n' "="     ;;
  '111110')  printf '%s\n' ">"     ;;
  '111111')  printf '%s\n' "?"     ;;
  '1000000') printf '%s\n' "@"     ;;
  '1000001') printf '%s\n' "A"     ;;
  '1000010') printf '%s\n' "B"     ;;
  '1000011') printf '%s\n' "C"     ;;
  '1000100') printf '%s\n' "D"     ;;
  '1000101') printf '%s\n' "E"     ;;
  '1000110') printf '%s\n' "F"     ;;
  '1000111') printf '%s\n' "G"     ;;
  '1001000') printf '%s\n' "H"     ;;
  '1001001') printf '%s\n' "I"     ;;
  '1001010') printf '%s\n' "J"     ;;
  '1001011') printf '%s\n' "K"     ;;
  '1001100') printf '%s\n' "L"     ;;
  '1001101') printf '%s\n' "M"     ;;
  '1001110') printf '%s\n' "N"     ;;
  '1001111') printf '%s\n' "O"     ;;
  '1010000') printf '%s\n' "P"     ;;
  '1010001') printf '%s\n' "Q"     ;;
  '1010010') printf '%s\n' "R"     ;;
  '1010011') printf '%s\n' "S"     ;;
  '1010100') printf '%s\n' "T"     ;;
  '1010101') printf '%s\n' "U"     ;;
  '1010110') printf '%s\n' "V"     ;;
  '1010111') printf '%s\n' "W"     ;;
  '1011000') printf '%s\n' "X"     ;;
  '1011001') printf '%s\n' "Y"     ;;
  '1011010') printf '%s\n' "Z"     ;;
  '1011011') printf '%s\n' "["     ;;
  '1011100') printf '%s\n' "\\"    ;;
  '1011101') printf '%s\n' "]"     ;;
  '1011110') printf '%s\n' "^"     ;;
  '1011111') printf '%s\n' "_"     ;;
  '1100000') printf '%s\n' "\`"    ;;
  '1100001') printf '%s\n' "a"     ;;
  '1100010') printf '%s\n' "b"     ;;
  '1100011') printf '%s\n' "c"     ;;
  '1100100') printf '%s\n' "d"     ;;
  '1100101') printf '%s\n' "e"     ;;
  '1100110') printf '%s\n' "f"     ;;
  '1100111') printf '%s\n' "g"     ;;
  '1101000') printf '%s\n' "h"     ;;
  '1101001') printf '%s\n' "i"     ;;
  '1101010') printf '%s\n' "j"     ;;
  '1101011') printf '%s\n' "k"     ;;
  '1101100') printf '%s\n' "l"     ;;
  '1101101') printf '%s\n' "m"     ;;
  '1101110') printf '%s\n' "n"     ;;
  '1101111') printf '%s\n' "o"     ;;
  '1110000') printf '%s\n' "p"     ;;
  '1110001') printf '%s\n' "q"     ;;
  '1110010') printf '%s\n' "r"     ;;
  '1110011') printf '%s\n' "s"     ;;
  '1110100') printf '%s\n' "t"     ;;
  '1110101') printf '%s\n' "u"     ;;
  '1110110') printf '%s\n' "v"     ;;
  '1110111') printf '%s\n' "w"     ;;
  '1111000') printf '%s\n' "x"     ;;
  '1111001') printf '%s\n' "y"     ;;
  '1111010') printf '%s\n' "z"     ;;
  '1111011') printf '%s\n' "{"     ;;
  '1111100') printf '%s\n' "|"     ;;
  '1111101') printf '%s\n' "}"     ;;
  '1111110') printf '%s\n' "~"     ;;
  '1111111') printf '%s\n' "DEL"   ;;
esac
