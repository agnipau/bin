#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given octal value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore ottale da convertire in ASCII.\\e[0m"

case "$1" in
  '0')   printf '%s\n' "NUL"   ;;
  '1')   printf '%s\n' "SOH"   ;;
  '2')   printf '%s\n' "STX"   ;;
  '3')   printf '%s\n' "ETX"   ;;
  '4')   printf '%s\n' "EOT"   ;;
  '5')   printf '%s\n' "ENQ"   ;;
  '6')   printf '%s\n' "ACK"   ;;
  '7')   printf '%s\n' "BEL"   ;;
  '10')  printf '%s\n' "BS"    ;;
  '11')  printf '%s\n' "TAB"   ;;
  '12')  printf '%s\n' "LF"    ;;
  '13')  printf '%s\n' "VT"    ;;
  '14')  printf '%s\n' "FF"    ;;
  '15')  printf '%s\n' "CR"    ;;
  '16')  printf '%s\n' "SO"    ;;
  '17')  printf '%s\n' "SI"    ;;
  '20')  printf '%s\n' "DLE"   ;;
  '21')  printf '%s\n' "DC1"   ;;
  '22')  printf '%s\n' "DC2"   ;;
  '23')  printf '%s\n' "DC3"   ;;
  '24')  printf '%s\n' "DC4"   ;;
  '25')  printf '%s\n' "NAK"   ;;
  '26')  printf '%s\n' "SYN"   ;;
  '27')  printf '%s\n' "ETB"   ;;
  '30')  printf '%s\n' "CAN"   ;;
  '31')  printf '%s\n' "EM"    ;;
  '32')  printf '%s\n' "SUB"   ;;
  '33')  printf '%s\n' "ESC"   ;;
  '34')  printf '%s\n' "FS"    ;;
  '35')  printf '%s\n' "GS"    ;;
  '36')  printf '%s\n' "RS"    ;;
  '37')  printf '%s\n' "US"    ;;
  '40')  printf '%s\n' "Space" ;;
  '41')  printf '%s\n' "!"     ;;
  '42')  printf '%s\n' "\""    ;;
  '43')  printf '%s\n' "#"     ;;
  '44')  printf '%s\n' "$"     ;;
  '45')  printf '%s\n' "%"     ;;
  '46')  printf '%s\n' "&"     ;;
  '47')  printf '%s\n' "'"     ;;
  '50')  printf '%s\n' "("     ;;
  '51')  printf '%s\n' ")"     ;;
  '52')  printf '%s\n' "*"     ;;
  '53')  printf '%s\n' "+"     ;;
  '54')  printf '%s\n' ","     ;;
  '55')  printf '%s\n' "-"     ;;
  '56')  printf '%s\n' "."     ;;
  '57')  printf '%s\n' "/"     ;;
  '60')  printf '%s\n' "0"     ;;
  '61')  printf '%s\n' "1"     ;;
  '62')  printf '%s\n' "2"     ;;
  '63')  printf '%s\n' "3"     ;;
  '64')  printf '%s\n' "4"     ;;
  '65')  printf '%s\n' "5"     ;;
  '66')  printf '%s\n' "6"     ;;
  '67')  printf '%s\n' "7"     ;;
  '70')  printf '%s\n' "8"     ;;
  '71')  printf '%s\n' "9"     ;;
  '72')  printf '%s\n' ":"     ;;
  '73')  printf '%s\n' ";"     ;;
  '74')  printf '%s\n' "<"     ;;
  '75')  printf '%s\n' "="     ;;
  '76')  printf '%s\n' ">"     ;;
  '77')  printf '%s\n' "?"     ;;
  '100') printf '%s\n' "@"     ;;
  '101') printf '%s\n' "A"     ;;
  '102') printf '%s\n' "B"     ;;
  '103') printf '%s\n' "C"     ;;
  '104') printf '%s\n' "D"     ;;
  '105') printf '%s\n' "E"     ;;
  '106') printf '%s\n' "F"     ;;
  '107') printf '%s\n' "G"     ;;
  '110') printf '%s\n' "H"     ;;
  '111') printf '%s\n' "I"     ;;
  '112') printf '%s\n' "J"     ;;
  '113') printf '%s\n' "K"     ;;
  '114') printf '%s\n' "L"     ;;
  '115') printf '%s\n' "M"     ;;
  '116') printf '%s\n' "N"     ;;
  '117') printf '%s\n' "O"     ;;
  '120') printf '%s\n' "P"     ;;
  '121') printf '%s\n' "Q"     ;;
  '122') printf '%s\n' "R"     ;;
  '123') printf '%s\n' "S"     ;;
  '124') printf '%s\n' "T"     ;;
  '125') printf '%s\n' "U"     ;;
  '126') printf '%s\n' "V"     ;;
  '127') printf '%s\n' "W"     ;;
  '130') printf '%s\n' "X"     ;;
  '131') printf '%s\n' "Y"     ;;
  '132') printf '%s\n' "Z"     ;;
  '133') printf '%s\n' "["     ;;
  '134') printf '%s\n' "\\"    ;;
  '135') printf '%s\n' "]"     ;;
  '136') printf '%s\n' "^"     ;;
  '137') printf '%s\n' "_"     ;;
  '140') printf '%s\n' "\`"    ;;
  '141') printf '%s\n' "a"     ;;
  '142') printf '%s\n' "b"     ;;
  '143') printf '%s\n' "c"     ;;
  '144') printf '%s\n' "d"     ;;
  '145') printf '%s\n' "e"     ;;
  '146') printf '%s\n' "f"     ;;
  '147') printf '%s\n' "g"     ;;
  '150') printf '%s\n' "h"     ;;
  '151') printf '%s\n' "i"     ;;
  '152') printf '%s\n' "j"     ;;
  '153') printf '%s\n' "k"     ;;
  '154') printf '%s\n' "l"     ;;
  '155') printf '%s\n' "m"     ;;
  '156') printf '%s\n' "n"     ;;
  '157') printf '%s\n' "o"     ;;
  '160') printf '%s\n' "p"     ;;
  '161') printf '%s\n' "q"     ;;
  '162') printf '%s\n' "r"     ;;
  '163') printf '%s\n' "s"     ;;
  '164') printf '%s\n' "t"     ;;
  '165') printf '%s\n' "u"     ;;
  '166') printf '%s\n' "v"     ;;
  '167') printf '%s\n' "w"     ;;
  '170') printf '%s\n' "x"     ;;
  '171') printf '%s\n' "y"     ;;
  '172') printf '%s\n' "z"     ;;
  '173') printf '%s\n' "{"     ;;
  '174') printf '%s\n' "|"     ;;
  '175') printf '%s\n' "}"     ;;
  '176') printf '%s\n' "~"     ;;
  '177') printf '%s\n' "DEL"   ;;
