  '90')  printf '%s\n' "Z"     ;;
  '91')  printf '%s\n' "["     ;;
  '92')  printf '%s\n' "\\"    ;;
  '93')  printf '%s\n' "]"     ;;
  '94')  printf '%s\n' "^"     ;;
  '95')  printf '%s\n' "_"     ;;
  '96')  printf '%s\n' "\`"    ;;
  '97')  printf '%s\n' "a"     ;;
  '98')  printf '%s\n' "b"     ;;
  '99')  printf '%s\n' "c"     ;;
  '100') printf '%s\n' "d"     ;;
  '101') printf '%s\n' "e"     ;;
  '102') printf '%s\n' "f"     ;;
  '103') printf '%s\n' "g"     ;;
  '104') printf '%s\n' "h"     ;;
  '105') printf '%s\n' "i"     ;;
  '106') printf '%s\n' "j"     ;;
  '107') printf '%s\n' "k"     ;;
  '108') printf '%s\n' "l"     ;;
  '109') printf '%s\n' "m"     ;;
  '110') printf '%s\n' "n"     ;;
  '111') printf '%s\n' "o"     ;;
  '112') printf '%s\n' "p"     ;;
  '113') printf '%s\n' "q"     ;;
  '114') printf '%s\n' "r"     ;;
  '115') printf '%s\n' "s"     ;;
  '116') printf '%s\n' "t"     ;;
  '117') printf '%s\n' "u"     ;;
  '118') printf '%s\n' "v"     ;;
  '119') printf '%s\n' "w"     ;;
  '120') printf '%s\n' "x"     ;;
  '121') printf '%s\n' "y"     ;;
  '122') printf '%s\n' "z"     ;;
  '123') printf '%s\n' "{"     ;;
  '124') printf '%s\n' "|"     ;;
  '125') printf '%s\n' "}"     ;;
