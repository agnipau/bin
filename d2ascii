#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given decimal value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore decimale da convertire in ASCII.\\e[0m"

case "$1" in
  '0')   printf '%s\n' "NUL"   ;;
  '1')   printf '%s\n' "SOH"   ;;
  '2')   printf '%s\n' "STX"   ;;
  '3')   printf '%s\n' "ETX"   ;;
  '4')   printf '%s\n' "EOT"   ;;
  '5')   printf '%s\n' "ENQ"   ;;
  '6')   printf '%s\n' "ACK"   ;;
  '7')   printf '%s\n' "BEL"   ;;
  '8')   printf '%s\n' "BS"    ;;
  '9')   printf '%s\n' "TAB"   ;;
  '10')  printf '%s\n' "LF"    ;;
  '11')  printf '%s\n' "VT"    ;;
  '12')  printf '%s\n' "FF"    ;;
  '13')  printf '%s\n' "CR"    ;;
  '14')  printf '%s\n' "SO"    ;;
  '15')  printf '%s\n' "SI"    ;;
  '16')  printf '%s\n' "DLE"   ;;
  '17')  printf '%s\n' "DC1"   ;;
  '18')  printf '%s\n' "DC2"   ;;
  '19')  printf '%s\n' "DC3"   ;;
  '20')  printf '%s\n' "DC4"   ;;
  '21')  printf '%s\n' "NAK"   ;;
  '22')  printf '%s\n' "SYN"   ;;
  '23')  printf '%s\n' "ETB"   ;;
  '24')  printf '%s\n' "CAN"   ;;
  '25')  printf '%s\n' "EM"    ;;
  '26')  printf '%s\n' "SUB"   ;;
  '27')  printf '%s\n' "ESC"   ;;
  '28')  printf '%s\n' "FS"    ;;
  '29')  printf '%s\n' "GS"    ;;
  '30')  printf '%s\n' "RS"    ;;
  '31')  printf '%s\n' "US"    ;;
  '32')  printf '%s\n' "Space" ;;
  '33')  printf '%s\n' "!"     ;;
  '34')  printf '%s\n' "\""    ;;
  '35')  printf '%s\n' "#"     ;;
  '36')  printf '%s\n' "$"     ;;
  '37')  printf '%s\n' "%"     ;;
  '38')  printf '%s\n' "&"     ;;
  '39')  printf '%s\n' "'"     ;;
  '40')  printf '%s\n' "("     ;;
  '41')  printf '%s\n' ")"     ;;
  '42')  printf '%s\n' "*"     ;;
  '43')  printf '%s\n' "+"     ;;
  '44')  printf '%s\n' ","     ;;
  '45')  printf '%s\n' "-"     ;;
  '46')  printf '%s\n' "."     ;;
  '47')  printf '%s\n' "/"     ;;
  '48')  printf '%s\n' "0"     ;;
  '49')  printf '%s\n' "1"     ;;
  '50')  printf '%s\n' "2"     ;;
  '51')  printf '%s\n' "3"     ;;
  '52')  printf '%s\n' "4"     ;;
  '53')  printf '%s\n' "5"     ;;
  '54')  printf '%s\n' "6"     ;;
  '55')  printf '%s\n' "7"     ;;
  '56')  printf '%s\n' "8"     ;;
  '57')  printf '%s\n' "9"     ;;
  '58')  printf '%s\n' ":"     ;;
  '59')  printf '%s\n' ";"     ;;
  '60')  printf '%s\n' "<"     ;;
  '61')  printf '%s\n' "="     ;;
  '62')  printf '%s\n' ">"     ;;
  '63')  printf '%s\n' "?"     ;;
  '64')  printf '%s\n' "@"     ;;
  '65')  printf '%s\n' "A"     ;;
  '66')  printf '%s\n' "B"     ;;
  '67')  printf '%s\n' "C"     ;;
  '68')  printf '%s\n' "D"     ;;
  '69')  printf '%s\n' "E"     ;;
  '70')  printf '%s\n' "F"     ;;
  '71')  printf '%s\n' "G"     ;;
  '72')  printf '%s\n' "H"     ;;
  '73')  printf '%s\n' "I"     ;;
  '74')  printf '%s\n' "J"     ;;
  '75')  printf '%s\n' "K"     ;;
  '76')  printf '%s\n' "L"     ;;
  '77')  printf '%s\n' "M"     ;;
  '78')  printf '%s\n' "N"     ;;
  '79')  printf '%s\n' "O"     ;;
  '80')  printf '%s\n' "P"     ;;
  '81')  printf '%s\n' "Q"     ;;
