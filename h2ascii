#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given hexadecimal value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore esadecimale da convertire in ASCII.\\e[0m"

case "$1" in
  '0')  printf '%s\n' "NUL"   ;;
  '1')  printf '%s\n' "SOH"   ;;
  '2')  printf '%s\n' "STX"   ;;
  '3')  printf '%s\n' "ETX"   ;;
  '4')  printf '%s\n' "EOT"   ;;
  '5')  printf '%s\n' "ENQ"   ;;
  '6')  printf '%s\n' "ACK"   ;;
  '7')  printf '%s\n' "BEL"   ;;
  '8')  printf '%s\n' "BS"    ;;
  '9')  printf '%s\n' "TAB"   ;;
  'A')  printf '%s\n' "LF"    ;;
  'B')  printf '%s\n' "VT"    ;;
  'C')  printf '%s\n' "FF"    ;;
  'D')  printf '%s\n' "CR"    ;;
  'E')  printf '%s\n' "SO"    ;;
  'F')  printf '%s\n' "SI"    ;;
  '10') printf '%s\n' "DLE"   ;;
  '11') printf '%s\n' "DC1"   ;;
  '12') printf '%s\n' "DC2"   ;;
  '13') printf '%s\n' "DC3"   ;;
  '14') printf '%s\n' "DC4"   ;;
  '15') printf '%s\n' "NAK"   ;;
  '16') printf '%s\n' "SYN"   ;;
  '17') printf '%s\n' "ETB"   ;;
  '18') printf '%s\n' "CAN"   ;;
  '19') printf '%s\n' "EM"    ;;
  '1A') printf '%s\n' "SUB"   ;;
  '1B') printf '%s\n' "ESC"   ;;
  '1C') printf '%s\n' "FS"    ;;
  '1D') printf '%s\n' "GS"    ;;
  '1E') printf '%s\n' "RS"    ;;
  '1F') printf '%s\n' "US"    ;;
  '20') printf '%s\n' "Space" ;;
  '21') printf '%s\n' "!"     ;;
  '22') printf '%s\n' "\""    ;;
  '23') printf '%s\n' "#"     ;;
  '24') printf '%s\n' "$"     ;;
  '25') printf '%s\n' "%"     ;;
  '26') printf '%s\n' "&"     ;;
  '27') printf '%s\n' "'"     ;;
  '28') printf '%s\n' "("     ;;
  '29') printf '%s\n' ")"     ;;
  '2A') printf '%s\n' "*"     ;;
  '2B') printf '%s\n' "+"     ;;
  '2C') printf '%s\n' ","     ;;
  '2D') printf '%s\n' "-"     ;;
  '2E') printf '%s\n' "."     ;;
  '2F') printf '%s\n' "/"     ;;
  '30') printf '%s\n' "0"     ;;
  '31') printf '%s\n' "1"     ;;
  '32') printf '%s\n' "2"     ;;
  '33') printf '%s\n' "3"     ;;
  '34') printf '%s\n' "4"     ;;
  '35') printf '%s\n' "5"     ;;
  '36') printf '%s\n' "6"     ;;
  '37') printf '%s\n' "7"     ;;
  '38') printf '%s\n' "8"     ;;
  '39') printf '%s\n' "9"     ;;
  '3A') printf '%s\n' ":"     ;;
  '3B') printf '%s\n' ";"     ;;
  '3C') printf '%s\n' "<"     ;;
  '3D') printf '%s\n' "="     ;;
  '3E') printf '%s\n' ">"     ;;
  '3F') printf '%s\n' "?"     ;;
  '40') printf '%s\n' "@"     ;;
  '41') printf '%s\n' "A"     ;;
  '42') printf '%s\n' "B"     ;;
  '43') printf '%s\n' "C"     ;;
  '44') printf '%s\n' "D"     ;;
  '45') printf '%s\n' "E"     ;;
  '46') printf '%s\n' "F"     ;;
  '47') printf '%s\n' "G"     ;;
  '48') printf '%s\n' "H"     ;;
  '49') printf '%s\n' "I"     ;;
  '4A') printf '%s\n' "J"     ;;
  '4B') printf '%s\n' "K"     ;;
  '4C') printf '%s\n' "L"     ;;
  '4D') printf '%s\n' "M"     ;;
  '4E') printf '%s\n' "N"     ;;
  '4F') printf '%s\n' "O"     ;;
  '50') printf '%s\n' "P"     ;;
  '51') printf '%s\n' "Q"     ;;
  '52') printf '%s\n' "R"     ;;
  '53') printf '%s\n' "S"     ;;
  '54') printf '%s\n' "T"     ;;
  '55') printf '%s\n' "U"     ;;
  '56') printf '%s\n' "V"     ;;
  '57') printf '%s\n' "W"     ;;
  '58') printf '%s\n' "X"     ;;
  '59') printf '%s\n' "Y"     ;;
  '5A') printf '%s\n' "Z"     ;;
  '5B') printf '%s\n' "["     ;;
  '5C') printf '%s\n' "\\"    ;;
  '5D') printf '%s\n' "]"     ;;
  '5E') printf '%s\n' "^"     ;;
  '5F') printf '%s\n' "_"     ;;
  '60') printf '%s\n' "\`"    ;;
  '61') printf '%s\n' "a"     ;;
  '62') printf '%s\n' "b"     ;;
  '63') printf '%s\n' "c"     ;;
  '64') printf '%s\n' "d"     ;;
  '65') printf '%s\n' "e"     ;;
  '66') printf '%s\n' "f"     ;;
  '67') printf '%s\n' "g"     ;;
  '68') printf '%s\n' "h"     ;;
  '69') printf '%s\n' "i"     ;;
  '6A') printf '%s\n' "j"     ;;
  '6B') printf '%s\n' "k"     ;;
  '6C') printf '%s\n' "l"     ;;
  '6D') printf '%s\n' "m"     ;;
  '6E') printf '%s\n' "n"     ;;
  '6F') printf '%s\n' "o"     ;;
  '70') printf '%s\n' "p"     ;;
  '71') printf '%s\n' "q"     ;;
  '72') printf '%s\n' "r"     ;;
  '73') printf '%s\n' "s"     ;;
  '74') printf '%s\n' "t"     ;;
  '75') printf '%s\n' "u"     ;;
  '76') printf '%s\n' "v"     ;;
  '77') printf '%s\n' "w"     ;;
  '78') printf '%s\n' "x"     ;;
  '79') printf '%s\n' "y"     ;;
  '7A') printf '%s\n' "z"     ;;
  '7B') printf '%s\n' "{"     ;;
  '7C') printf '%s\n' "|"     ;;
  '7D') printf '%s\n' "}"     ;;
  '7E') printf '%s\n' "~"     ;;
  '7F') printf '%s\n' "DEL"   ;;
esac
