#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given hexadecimal value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore esadecimale da convertire in ASCII.\\e[0m"

case "$1" in
  'NUL')   printf '%s\n' "0"  ;;
  'SOH')   printf '%s\n' "1"  ;;
  'STX')   printf '%s\n' "2"  ;;
  'ETX')   printf '%s\n' "3"  ;;
  'EOT')   printf '%s\n' "4"  ;;
  'ENQ')   printf '%s\n' "5"  ;;
  'ACK')   printf '%s\n' "6"  ;;
  'BEL')   printf '%s\n' "7"  ;;
  'BS')    printf '%s\n' "8"  ;;
  'TAB')   printf '%s\n' "9"  ;;
  'LF')    printf '%s\n' "A"  ;;
  'VT')    printf '%s\n' "B"  ;;
  'FF')    printf '%s\n' "C"  ;;
  'CR')    printf '%s\n' "D"  ;;
  'SO')    printf '%s\n' "E"  ;;
  'SI')    printf '%s\n' "F"  ;;
  'DLE')   printf '%s\n' "10" ;;
  'DC1')   printf '%s\n' "11" ;;
  'DC2')   printf '%s\n' "12" ;;
  'DC3')   printf '%s\n' "13" ;;
  'DC4')   printf '%s\n' "14" ;;
  'NAK')   printf '%s\n' "15" ;;
  'SYN')   printf '%s\n' "16" ;;
  'ETB')   printf '%s\n' "17" ;;
  'CAN')   printf '%s\n' "18" ;;
  'EM')    printf '%s\n' "19" ;;
  'SUB')   printf '%s\n' "1A" ;;
  'ESC')   printf '%s\n' "1B" ;;
  'FS')    printf '%s\n' "1C" ;;
  'GS')    printf '%s\n' "1D" ;;
  'RS')    printf '%s\n' "1E" ;;
  '1F')    printf '%s\n' "US" ;;
  'Space') printf '%s\n' "20" ;;
  '!')     printf '%s\n' "21" ;;
  '"')     printf '%s\n' "22" ;;
  '#')     printf '%s\n' "23" ;;
  '$')     printf '%s\n' "24" ;;
  '%')     printf '%s\n' "25" ;;
  '&')     printf '%s\n' "26" ;;
  "'")     printf '%s\n' "27" ;;
  '(')     printf '%s\n' "28" ;;
  ')')     printf '%s\n' "29" ;;
  '*')     printf '%s\n' "2A" ;;
  '+')     printf '%s\n' "2B" ;;
  ',')     printf '%s\n' "2C" ;;
  '-')     printf '%s\n' "2D" ;;
  '.')     printf '%s\n' "2E" ;;
  '/')     printf '%s\n' "2F" ;;
  '0')     printf '%s\n' "30" ;;
  '1')     printf '%s\n' "31" ;;
  '2')     printf '%s\n' "32" ;;
  '3')     printf '%s\n' "33" ;;
  '4')     printf '%s\n' "34" ;;
  '5')     printf '%s\n' "35" ;;
  '6')     printf '%s\n' "36" ;;
  '7')     printf '%s\n' "37" ;;
  '8')     printf '%s\n' "38" ;;
  '9')     printf '%s\n' "39" ;;
  ':')     printf '%s\n' "3A" ;;
  ';')     printf '%s\n' "3B" ;;
  '<')     printf '%s\n' "3C" ;;
  '=')     printf '%s\n' "3D" ;;
  '>')     printf '%s\n' "3E" ;;
  '?')     printf '%s\n' "3F" ;;
  '@')     printf '%s\n' "40" ;;
  'A')     printf '%s\n' "41" ;;
  'B')     printf '%s\n' "42" ;;
  'C')     printf '%s\n' "43" ;;
  'D')     printf '%s\n' "44" ;;
  'E')     printf '%s\n' "45" ;;
  'F')     printf '%s\n' "46" ;;
  'G')     printf '%s\n' "47" ;;
  'H')     printf '%s\n' "48" ;;
  'I')     printf '%s\n' "49" ;;
  'J')     printf '%s\n' "4A" ;;
  'K')     printf '%s\n' "4B" ;;
  'L')     printf '%s\n' "4C" ;;
  'M')     printf '%s\n' "4D" ;;
  'N')     printf '%s\n' "4E" ;;
  'O')     printf '%s\n' "4F" ;;
  'P')     printf '%s\n' "50" ;;
  'Q')     printf '%s\n' "51" ;;
  'R')     printf '%s\n' "52" ;;
  'S')     printf '%s\n' "53" ;;
  'T')     printf '%s\n' "54" ;;
  'U')     printf '%s\n' "55" ;;
  'V')     printf '%s\n' "56" ;;
  'W')     printf '%s\n' "57" ;;
  'X')     printf '%s\n' "58" ;;
  'Y')     printf '%s\n' "59" ;;
  'Z')     printf '%s\n' "5A" ;;
  '[')     printf '%s\n' "5B" ;;
  \\)      printf '%s\n' "5C" ;;
  ']')     printf '%s\n' "5D" ;;
  '^')     printf '%s\n' "5E" ;;
  '_')     printf '%s\n' "5F" ;;
  '\`')    printf '%s\n' "60" ;;
  'a')     printf '%s\n' "61" ;;
  'b')     printf '%s\n' "62" ;;
  'c')     printf '%s\n' "63" ;;
  'd')     printf '%s\n' "64" ;;
  'e')     printf '%s\n' "65" ;;
  'f')     printf '%s\n' "66" ;;
  'g')     printf '%s\n' "67" ;;
  'h')     printf '%s\n' "68" ;;
  'i')     printf '%s\n' "69" ;;
  'j')     printf '%s\n' "6A" ;;
  'k')     printf '%s\n' "6B" ;;
  'l')     printf '%s\n' "6C" ;;
  'm')     printf '%s\n' "6D" ;;
  'n')     printf '%s\n' "6E" ;;
  'o')     printf '%s\n' "6F" ;;
  'p')     printf '%s\n' "70" ;;
  'q')     printf '%s\n' "71" ;;
  'r')     printf '%s\n' "72" ;;
  's')     printf '%s\n' "73" ;;
  't')     printf '%s\n' "74" ;;
  'u')     printf '%s\n' "75" ;;
  'v')     printf '%s\n' "76" ;;
  'w')     printf '%s\n' "77" ;;
  'x')     printf '%s\n' "78" ;;
  'y')     printf '%s\n' "79" ;;
  'z')     printf '%s\n' "7A" ;;
  '{')     printf '%s\n' "7B" ;;
  '|')     printf '%s\n' "7C" ;;
  '}')     printf '%s\n' "7D" ;;
  '~')     printf '%s\n' "7E" ;;
  'DEL')   printf '%s\n' "7F" ;;
esac
