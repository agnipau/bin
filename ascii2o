#!/usr/bin/env bash
#
# Outputs the ASCII character that corresponds to the given octal value.

[[ -z "$1" ]] && printf '%b\n' "\\e[1;31m::\\e[0m \\e[1;37mMagari la prossima volta dammi un valore ottale da convertire in ASCII.\\e[0m"

case "$1" in
  'NUL')   printf '%s\n' "0"   ;;
  'SOH')   printf '%s\n' "1"   ;;
  'STX')   printf '%s\n' "2"   ;;
  'ETX')   printf '%s\n' "3"   ;;
  'EOT')   printf '%s\n' "4"   ;;
  'ENQ')   printf '%s\n' "5"   ;;
  'ACK')   printf '%s\n' "6"   ;;
  'BEL')   printf '%s\n' "7"   ;;
  'BS')    printf '%s\n' "10"  ;;
  'TAB')   printf '%s\n' "11"  ;;
  'LF')    printf '%s\n' "12"  ;;
  'VT')    printf '%s\n' "13"  ;;
  'FF')    printf '%s\n' "14"  ;;
  'CR')    printf '%s\n' "15"  ;;
  'SO')    printf '%s\n' "16"  ;;
  'SI')    printf '%s\n' "17"  ;;
  'DLE')   printf '%s\n' "20"  ;;
  'DC1')   printf '%s\n' "21"  ;;
  'DC2')   printf '%s\n' "22"  ;;
  'DC3')   printf '%s\n' "23"  ;;
  'DC4')   printf '%s\n' "24"  ;;
  'NAK')   printf '%s\n' "25"  ;;
  'SYN')   printf '%s\n' "26"  ;;
  'ETB')   printf '%s\n' "27"  ;;
  'CAN')   printf '%s\n' "30"  ;;
  'EM')    printf '%s\n' "31"  ;;
  'SUB')   printf '%s\n' "32"  ;;
  'ESC')   printf '%s\n' "33"  ;;
  'FS')    printf '%s\n' "34"  ;;
  'GS')    printf '%s\n' "35"  ;;
  'RS')    printf '%s\n' "36"  ;;
  'US')    printf '%s\n' "37"  ;;
  'Space') printf '%s\n' "40"  ;;
  '!')     printf '%s\n' "41"  ;;
  '"')     printf '%s\n' "42"  ;;
  '#')     printf '%s\n' "43"  ;;
  '$')     printf '%s\n' "44"  ;;
  '%')     printf '%s\n' "45"  ;;
  '&')     printf '%s\n' "46"  ;;
  "'")     printf '%s\n' "47"  ;;
  '(')     printf '%s\n' "50"  ;;
  ')')     printf '%s\n' "51"  ;;
  '*')     printf '%s\n' "52"  ;;
  '+')     printf '%s\n' "53"  ;;
  ',')     printf '%s\n' "54"  ;;
  '-')     printf '%s\n' "55"  ;;
  '.')     printf '%s\n' "56"  ;;
  '/')     printf '%s\n' "57"  ;;
  '0')     printf '%s\n' "60"  ;;
  '1')     printf '%s\n' "61"  ;;
  '2')     printf '%s\n' "62"  ;;
  '3')     printf '%s\n' "63"  ;;
  '4')     printf '%s\n' "64"  ;;
  '5')     printf '%s\n' "65"  ;;
  '6')     printf '%s\n' "66"  ;;
  '7')     printf '%s\n' "67"  ;;
  '8')     printf '%s\n' "70"  ;;
  '9')     printf '%s\n' "71"  ;;
  ':')     printf '%s\n' "72"  ;;
  ';')     printf '%s\n' "73"  ;;
  '<')     printf '%s\n' "74"  ;;
  '=')     printf '%s\n' "75"  ;;
  '>')     printf '%s\n' "76"  ;;
  '?')     printf '%s\n' "77"  ;;
  '@')     printf '%s\n' "100" ;;
  'A')     printf '%s\n' "101" ;;
  'B')     printf '%s\n' "102" ;;
  'C')     printf '%s\n' "103" ;;
  'D')     printf '%s\n' "104" ;;
  'E')     printf '%s\n' "105" ;;
  'F')     printf '%s\n' "106" ;;
  'G')     printf '%s\n' "107" ;;
  'H')     printf '%s\n' "110" ;;
  'I')     printf '%s\n' "111" ;;
  'J')     printf '%s\n' "112" ;;
  'K')     printf '%s\n' "113" ;;
  'L')     printf '%s\n' "114" ;;
  'M')     printf '%s\n' "115" ;;
  'N')     printf '%s\n' "116" ;;
  'O')     printf '%s\n' "117" ;;
  'P')     printf '%s\n' "120" ;;
  'Q')     printf '%s\n' "121" ;;
  'R')     printf '%s\n' "122" ;;
  'S')     printf '%s\n' "123" ;;
  'T')     printf '%s\n' "124" ;;
  'U')     printf '%s\n' "125" ;;
  'V')     printf '%s\n' "126" ;;
  'W')     printf '%s\n' "127" ;;
  'X')     printf '%s\n' "130" ;;
  'Y')     printf '%s\n' "131" ;;
  'Z')     printf '%s\n' "132" ;;
  '[')     printf '%s\n' "133" ;;
  \\)      printf '%s\n' "134" ;;
  ']')     printf '%s\n' "135" ;;
  '^')     printf '%s\n' "136" ;;
  '_')     printf '%s\n' "137" ;;
  '\`')    printf '%s\n' "140" ;;
  'a')     printf '%s\n' "141" ;;
  'b')     printf '%s\n' "142" ;;
  'c')     printf '%s\n' "143" ;;
  'd')     printf '%s\n' "144" ;;
  'e')     printf '%s\n' "145" ;;
  'f')     printf '%s\n' "146" ;;
  'g')     printf '%s\n' "147" ;;
  'h')     printf '%s\n' "150" ;;
  'i')     printf '%s\n' "151" ;;
  'j')     printf '%s\n' "152" ;;
  'k')     printf '%s\n' "153" ;;
  'l')     printf '%s\n' "154" ;;
  'm')     printf '%s\n' "155" ;;
  'n')     printf '%s\n' "156" ;;
  'o')     printf '%s\n' "157" ;;
  'p')     printf '%s\n' "160" ;;
  'q')     printf '%s\n' "161" ;;
  'r')     printf '%s\n' "162" ;;
  's')     printf '%s\n' "163" ;;
  't')     printf '%s\n' "164" ;;
  'u')     printf '%s\n' "165" ;;
  'v')     printf '%s\n' "166" ;;
  'w')     printf '%s\n' "167" ;;
  'x')     printf '%s\n' "170" ;;
  'y')     printf '%s\n' "171" ;;
  'z')     printf '%s\n' "172" ;;
  '{')     printf '%s\n' "173" ;;
  '|')     printf '%s\n' "174" ;;
  '}')     printf '%s\n' "175" ;;
  '~')     printf '%s\n' "176" ;;
  'DEL')   printf '%s\n' "177" ;;
esac
